{
  "kind": "collectionType",
  "collectionName": "eventos_publicos",
  "info": {
    "singularName": "evento-publico",
    "pluralName": "eventos-publicos",
    "displayName": "Eventos Públicos",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "date": {
      "type": "date",
      "required": true
    },
    "event_type": {
      "type": "enumeration",
      "enum": [
        "Audiência Pública",
        "Consulta Pública"
      ],
      "default": "Audiência Pública",
      "required": true
    },
    "calendar": {
      "type": "component",
      "repeatable": true,
      "component": "event.event"
    },
    "notify": {
      "type": "richtext"
    },
    "place": {
      "type": "richtext",
      "default": "Auditório da AGERBA, sito na 4ª Avenida, nº 435, térreo - CAB - Salvador - Bahia."
    },
    "Documentos": {
      "type": "component",
      "repeatable": true,
      "component": "arquivos.arquivos",
      "required": false
    },
    "meetLink": {
      "type": "text"
    },
    "onlyRemote": {
      "type": "boolean",
      "default": false
    },
    "finalReport": {
      "type": "component",
      "repeatable": true,
      "component": "arquivos.arquivos"
    },
    "justification": {
      "type": "string"
    }
  }
}
